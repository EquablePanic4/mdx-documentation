<?xml version="1.0" encoding="UTF-8"?>
<definitions>
    <!--md default -->
    <cmdx type="preline" pattern="p...p" match="all" />
    <cmdx type="preline" pattern="h1...h1" match="#" css="cmdx" />
    <cmdx type="preline" pattern="h2...h2" match="##" css="cmdx" />
    <cmdx type="preline" pattern="h3...h3" match="###" css="cmdx" />
    <cmdx type="preline" pattern="h4...h4" match="####" css="cmdx" />
    <cmdx type="preline" pattern="h5...h5" match="#####" css="cmdx" />
    <cmdx type="preline" pattern="h6...h6" match="######" css="cmdx" />
    <cmdx type="preline" pattern="img alt{0} src{1} cmdx-size{2}" match="!" css="cmdx" />
    <cmdx type="preline" pattern="ul...li...ul" match="*" css="cmdx" />

    <!--CMDX Extra-->
    <cmdx type="preline" pattern="ol...li...ol" match="-" css="cmdx" />
    <cmdx type="alias" pattern="strong" match="[s]" />
    <cmdx type="alias" pattern="/strong" match="[/s]" />
    <cmdx type="alias" pattern="/strong" match="[.s]" />
</definitions>